    "idb": "^8.0.3",
    "natural": "^8.1.0",
    "pinia": "^2.1.7",
    "sentiment": "^5.0.2",
    "three": "^0.160.1",
    "tone": "^14.7.77",
    "vite-plugin-pwa": "^1.1.0",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0",
    "web-vitals": "^5.1.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.4",
    "autoprefixer": "^10.4.19",
